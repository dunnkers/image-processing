function h = IPhistogram(I, bins)
% Image size
M = size(I, 1); % height
N = size(I, 2); % width

% Histogram bins
h = zeros(1, 2^8); % 8-bit image
bins = floor(250 / (256/10))

% Count pixels of some intensity value, `r_k`, in the bins
for i = 1:M
    for j = 1:N
        k = floor(I(i, j));
        r_k = floor(I(i, j)) + 1; % use floor() for integer casting

        n_k = h(r_k) + 1;
        h(r_k) = n_k;
    end
end
end

